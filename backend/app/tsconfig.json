{
	"compilerOptions": {
		"target": "ES2020",
		"module": "esnext",
		"moduleResolution": "bundler",
		"rootDir": "src",
		"outDir": "dist",
		"strict": true,
		"esModuleInterop": true,
		"forceConsistentCasingInFileNames": true,
		"skipLibCheck": true,
		"baseUrl": "./src",
		"paths": {
			"@/*": ["*"]
		}
	},
	"include": [
		"src"
	]
}